{
    "name": "andregoncalvesdev/laravelnews",
    "type": "library",
    "description": "News Package",
    "keywords": [
        "andregoncalvesdev",
        "laravelnews",
        "laravel"
    ],
    "homepage": "https://github.com/andregoncalvesdev/laravelnews",
    "license": "MIT",
    "authors": [
        {
            "name": "andregoncalvesdev"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "AndreGoncalvesDev\\LaravelNews\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndreGoncalvesDev\\LaravelNews\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
